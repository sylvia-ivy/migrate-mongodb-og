  "devDependencies": {
    "chai": "^4.3.6",
    "coveralls": "^3.1.1",
    "eslint": "^8.12.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-mocha": "^10.0.3",
    "mocha": "^9.2.2",
    "nyc": "^15.1.0",
    "proxyquire": "^2.1.3",
    "sinon": "^13.0.1"
  },
  "eslintConfig": {
    "extends": [
      "airbnb-base",
      "prettier"
    ],
    "parserOptions": {
      "ecmaVersion": 2018
    }
  },
  "bugs": {
    "url": "https://github.com/sylvia-ivy/migrate-mongodb-og/issues"
  },
